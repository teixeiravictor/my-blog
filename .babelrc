{
  "presets": ["next/babel"],
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "ssr": true,
        "displayName": true
      }
    ],
    [
      "prismjs",
      {
        "languages": [
          "javascript",
          "css",
          "typescript",
          "json",
          "jsx",
          "markdown",
          "bash",
          "yaml"
        ],
        "plugins": ["line-numbers"],
        "theme": "tomorrow",
        "css": true
      }
    ]
  ]
}
